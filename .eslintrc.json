{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "plugins": [ "jest" ],
    "extends": ["eslint:recommended", "plugin:jest/recommended"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["warn" ,"double"],
        "no-var":"warn",
        "no-use-before-define":"error",
        "no-extra-semi": "error",
        "jest/no-disabled-tests": "warn",
        "jest/no-identical-title": "error"
    }
}
